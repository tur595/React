\n};\r\n\r\nconst Details = styled(motion.div)`\r\n  color: white;\r\n`;\r\n\r\nconst HeadLine = styled.div`\r\n  min-height: 90vh;\r\n  padding-top: 20vh;\r\n  position: relative;\r\n  h2 {\r\n    position: absolute;\r\n    top: 10%;\r\n    left: 50%;\r\n    transform: translate(-50%, -10%);\r\n  }\r\n  img {\r\n    width: 100%;\r\n    height: 70vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\nconst Awards = styled.div`\r\n  min-height: 80vh;\r\n  display: flex;\r\n  margin: 5rem 10rem;\r\n  align-items: center;\r\n  justify-content: space-around;\r\n`;\r\n\r\nconst AwardStyle = styled.div`\r\n  padding: 5rem;\r\n  h3 {\r\n    font-size: 2rem;\r\n  }\r\n  .line {\r\n    width: 100%;\r\n    background: #23d997;\r\n    height: 0.5rem;\r\n    margin: 1rem 0rem;\r\n  }\r\n  p {\r\n    padding: 2rem 0rem;\r\n  }\r\n`;\r\n\r\nconst ImageDisplay = styled.div`\r\n  min-height: 50vh;\r\n  img {\r\n    width: 100%;\r\n    height: 100vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\n//Award component\r\n\r\nconst Award = ({ title, description }) => {\r\n  return (\r\n    <AwardStyle>\r\n      <h3>{title}</h3>\r\n      <div className=\"line\"></div>\r\n      <p>{description}</p>\r\n    </AwardStyle>\r\n  );\r\n};\r\n\r\nexport default MovieDetail;\r\n","/mnt/c/Users/tur59/Documents/Programming/React/capture-website/src/movieState.js",[],"/mnt/c/Users/tur59/Documents/Programming/React/capture-website/src/animation.js",[],"/mnt/c/Users/tur59/Documents/Programming/React/capture-website/src/components/Wave.js",[],"/mnt/c/Users/tur59/Documents/Programming/React/capture-website/src/components/Toggle.js",[],"/mnt/c/Users/tur59/Documents/Programming/React/capture-website/src/components/useScroll.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":59,"column":39,"nodeType":"88","endLine":59,"endColumn":45},{"ruleId":"89","severity":1,"message":"90","line":11,"column":10,"nodeType":"91","messageId":"92","endLine":